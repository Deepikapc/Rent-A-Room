<%# if @bookings_error %>
	<!--<p> "Booking is Full" </p> -->
<%# end %>
<%= form_for @booking do |f| %>
	<% if @booking.errors.any? %>
		<ul>
		<% @booking.errors.full_messages.each do |msg| %>
			<li><%= msg %></li>
		<% end %>
		<ul>
	<% end %>
	<%= f.label :start_date %>
	<%= f.text_field :start_date %><br/>
	<%= f.label :end_date %>
	<%= f.text_field :end_date %><br/>
	
	<%= f.submit %>
<% end %>


<script>
  $(document).ready(function () {
    $("#booking_start_date").datepicker({dateFormat: "yy-mm-dd"});
    $("#booking_end_date").datepicker({dateFormat: "yy-mm-dd"});
    
  });
</script>

